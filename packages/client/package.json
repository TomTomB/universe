{
  "name": "@universe/client",
  "version": "0.0.1",
  "private": true,
  "main": "dist/main/index.cjs",
  "description": "Universe client",
  "author": "Tom-Rune Bornholdt",
  "browserslist": [
    "Chrome 96"
  ],
  "scripts": {
    "postinstall": "yarn gen:build-vars",
    "build": "node ./scripts/build.js",
    "package": "node ./scripts/package.js",
    "serve": "node ./scripts/serve.js",
    "gen:build-vars": "node ./scripts/generate-build-vars.js",
    "type-check": "yarn type-check:main && yarn type-check:preload && yarn type-check:renderer",
    "type-check:main": "tsc -p ./modules/main/tsconfig.json --noEmit",
    "type-check:preload": "tsc -p ./modules/preload/tsconfig.json --noEmit",
    "type-check:renderer": "vue-tsc -p ./modules/renderer/tsconfig.json --noEmit"
  },
  "dependencies": {
    "@sentry/electron": "2.5.4",
    "@vueuse/core": "7.5.1",
    "node-fetch": "3.1.0",
    "chokidar": "3.5.2",
    "electron-log": "4.4.4",
    "electron-updater": "4.6.5",
    "find-process": "1.4.7",
    "gl-matrix": "3.4.3",
    "firebase": "9.6.1",
    "pinia": "2.0.9",
    "shortid": "2.2.16",
    "vue": "3.2.26",
    "vue-router": "4.0.12",
    "vue-i18n": "9.1.9",
    "xstate": "4.27.0",
    "ws": "8.4.0"
  },
  "electronDependencies": [
    "@sentry/electron",
    "electron-log",
    "electron-updater",
    "electron",
    "find-process",
    "ws"
  ],
  "devDependencies": {
    "@types/electron-devtools-installer": "2.2.1",
    "@types/ws": "8.2.2",
    "@types/node-fetch": "3.0.3",
    "electron": "16.0.6",
    "electron-builder": "22.14.5",
    "electron-devtools-installer": "3.2.0",
    "eslint-plugin-vue": "8.2.0",
    "@types/shortid": "0.0.29",
    "@vitejs/plugin-vue": "2.0.1",
    "vue-tsc": "0.30.2",
    "sass": "1.45.2"
  }
}

{
  "name": "@universe/client",
  "version": "0.0.1",
  "private": true,
  "main": "dist/main/index.cjs",
  "description": "Universe client",
  "author": "Tom-Rune Bornholdt",
  "browserslist": [
    "Chrome 96"
  ],
  "scripts": {
    "postinstall": "yarn gen:build-vars",
    "build": "node ./scripts/build.js",
    "package": "node ./scripts/package.js",
    "serve": "node ./scripts/serve.js",
    "gen:build-vars": "node ./scripts/generate-build-vars.js",
    "type-check": "yarn type-check:main && yarn type-check:preload && yarn type-check:renderer && yarn type-check:renderer-vue",
    "type-check:main": "tsc -p ./modules/main/tsconfig.json --noEmit",
    "type-check:preload": "tsc -p ./modules/preload/tsconfig.json --noEmit",
    "type-check:renderer": "tsc -p ./modules/renderer/tsconfig.json --noEmit",
    "type-check:renderer-vue": "vue-tsc -p ./modules/renderer-vue/tsconfig.json --noEmit"
  },
  "dependencies": {
    "@sentry/electron": "2.5.4",
    "@reduxjs/toolkit": "1.7.1",
    "@xstate/react": "1.6.3",
    "node-fetch": "3.1.0",
    "chokidar": "3.5.2",
    "classnames": "2.3.1",
    "electron-log": "4.4.3",
    "electron-updater": "4.6.5",
    "find-process": "1.4.7",
    "focus-trap-react": "8.9.0",
    "gl-matrix": "3.4.3",
    "i18next": "21.6.3",
    "i18next-http-backend": "1.3.1",
    "firebase": "9.6.1",
    "pinia": "2.0.8",
    "react-redux": "7.2.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.22.2",
    "react-i18next": "11.15.1",
    "react-popper-tooltip": "4.3.1",
    "react-router-dom": "6.2.1",
    "react-spring": "9.3.2",
    "shortid": "2.2.16",
    "styled-components": "5.3.3",
    "vue": "3.2.26",
    "vue-router": "4.0.12",
    "vue-i18n": "9.1.9",
    "xstate": "4.26.1",
    "yup": "0.32.11",
    "ws": "8.3.0"
  },
  "electronDependencies": [
    "@sentry/electron",
    "electron-log",
    "electron-updater",
    "electron",
    "find-process",
    "ws"
  ],
  "devDependencies": {
    "@types/electron-devtools-installer": "2.2.0",
    "@types/ws": "8.2.2",
    "@types/node-fetch": "3.0.3",
    "electron": "16.0.5",
    "electron-builder": "22.14.5",
    "electron-devtools-installer": "3.2.0",
    "eslint-plugin-vue": "8.2.0",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "5.1.18",
    "@vitejs/plugin-react": "1.1.3",
    "@vitejs/plugin-vue": "2.0.1",
    "vue-tsc": "0.29.8",
    "sass": "1.45.0",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11"
  }
}

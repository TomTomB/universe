{
  "extends": "../../../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "paths": {
      "/@/*": ["./src/*"]
    },
    "lib": ["ESNext", "dom", "dom.iterable"]
  },

  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "types/**/*.d.ts",
    "../../../../types/**/*.d.ts",
    "../preload/types/electron-api.d.ts"
  ]
}
